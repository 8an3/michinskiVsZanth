{
  "dotenv.enableAutocloaking": true,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [
    120
  ],
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.encoding": "utf8",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "peacock.color": "#be0000",
  "search.exclude": {
    ".cache/**": true,
    "build/**": true,
    "node_modules/**": true,
    "public/build/**": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#d62c2c",
    "activityBar.background": "#d62c2c",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f9e6f2",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#d62c2c",
    "statusBar.background": "#be0000",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#297aa3",
    "statusBarItem.remoteBackground": "#be0000",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#be0000",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#be000099",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "automaton.list": {
    "commentDown": {
      "description": "Fold MOTHERFUCKER!@",
      "action": [
        "editor.foldLevel1",
        "editor.foldLevel2"
      ]
    },
  },
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
